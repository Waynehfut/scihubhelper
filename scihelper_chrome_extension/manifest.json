{
    "manifest_version": 2,
    "name": "SCI Helper",
    "version": "1.0",
    "description": "Help you find the accessible paper",
    "permissions": [
        "tabs"
    ],
    "icons": {
        "16": "images/16x16.png",
        "48": "images/48x48.png",
        "128": "images/128x128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "js/add_scihub_btn.js"
            ],
            "css": [
                "css/content.css"
            ]
        }
    ]
}